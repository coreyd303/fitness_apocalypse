<h1> Profile </h1>
<br>
<div class="col-md-8">
  <%= form_for @profile do |f| %>
    <div class="form-group">
      <%= f.label :firstname %>
      <%= f.text_field :firstname, class: 'form-control' %>
    </div>
    <div class="form-group">
      <%= f.label :lastname %>
      <%= f.text_field :lastname, class: 'form-control' %>
    </div>

  <% end %>
</div>